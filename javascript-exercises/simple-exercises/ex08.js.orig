var findLongestWord = function(wordList){
  if (!wordList) throw new Error('findLongestWord: Null entry');
  var longest = 0;
  for (var i = 0; i < wordList.length; i++) {
    if (wordList[i].length > longest) {
      longest = wordList[i].length;
    }
  }
  return longest;
